{
  "name": "ddalab-tauri",
  "version": "1.0.19",
  "description": "DDALAB Desktop Application built with Next.js and Tauri",
  "private": true,
  "scripts": {
    "dev": "PORT=3003 next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "tauri": "tauri",
    "tauri:dev": "tauri dev",
    "tauri:build": "tauri build",
    "tauri:icon": "tauri icon",
    "desktop:dev": "npm run tauri:dev",
    "desktop:build": "npm run build && npm run tauri:build",
    "desktop:with-api": "./start-with-api.sh",
    "api:start": "../../scripts/start-api-only.sh up",
    "api:stop": "../../scripts/start-api-only.sh down",
    "api:logs": "../../scripts/start-api-only.sh logs",
    "test": "echo \"No tests specified\" && exit 0",
    "release": "npm run build && npm run tauri:build && echo 'Build complete! Check src-tauri/target/release/bundle/'",
    "release:mac": "npm run build && tauri build --target universal-apple-darwin",
    "release:test": "./build-test-dmg.sh",
    "release:github": "../../scripts/build-and-release.sh"
  },
  "dependencies": {
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-radio-group": "^1.3.8",
    "@radix-ui/react-select": "^1.2.2",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@tauri-apps/api": "^2.8.0",
    "@tauri-apps/plugin-dialog": "^2.4.0",
    "@tauri-apps/plugin-fs": "^2.4.2",
    "@tauri-apps/plugin-shell": "^2.0.0",
    "axios": "^1.12.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^3.6.0",
    "lucide-react": "^0.544.0",
    "next": "^15.5.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-grid-layout": "^1.5.2",
    "react-resizable": "^3.0.5",
    "recharts": "^2.15.4",
    "tailwind-merge": "^2.6.0",
    "tailwindcss-animate": "^1.0.7",
    "uplot": "^1.6.32",
    "uplot-react": "^1.2.4",
    "zustand": "^4.5.7"
  },
  "devDependencies": {
    "@tauri-apps/cli": "^2.8.4",
    "@types/node": "^20.19.19",
    "@types/react": "^18.3.25",
    "@types/react-dom": "^18.3.7",
    "@types/react-grid-layout": "^1.3.5",
    "autoprefixer": "^10.4.21",
    "eslint": "^8.57.1",
    "eslint-config-next": "^15.5.4",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.18",
    "typescript": "^5.9.3"
  }
}
