{
  "name": "ddalab-tauri",
  "version": "1.2.0",
  "description": "DDALAB Desktop Application built with Next.js and Tauri",
  "private": true,
  "scripts": {
    "dev": "PORT=3003 next dev --turbo",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "tauri": "tauri",
    "tauri:dev": "tauri dev",
    "tauri:dev:serial": "tauri dev",
    "tauri:build": "tauri build",
    "tauri:icon": "tauri icon",
    "fmt": "prettier --write \"./**/*.{js,jsx,ts,tsx,json,css,scss,html,md}\" && cd src-tauri && cargo fmt --all",
    "desktop:dev": "bun run tauri:dev",
    "desktop:build": "bun run build && bun run tauri:build",
    "test": "cd src-tauri && cargo test",
    "test:e2e": "bunx playwright test",
    "test:e2e:ui": "bunx playwright test --ui",
    "test:e2e:headed": "bunx playwright test --headed",
    "test:e2e:debug": "bunx playwright test --debug",
    "test:e2e:report": "bunx playwright show-report e2e-report",
    "test:e2e:chromium": "bunx playwright test --project=chromium",
    "test:e2e:coverage": "COVERAGE=true bunx playwright test",
    "test:e2e:coverage:open": "open coverage-e2e/index.html",
    "test:api-server:build": "cd src-tauri && cargo build --bin api-server --release",
    "test:api-server:start": "./src-tauri/target/release/api-server --data-dir ../../data --port 8765",
    "release": "bun run build && bun run tauri:build && echo 'Build complete! Check src-tauri/target/release/bundle/'",
    "release:mac": "bun run build && tauri build --target universal-apple-darwin",
    "release:test": "./build-test-dmg.sh",
    "release:github": "../../scripts/build-and-release.sh",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build -o docs-dist/storybook",
    "docs:api": "typedoc",
    "docs:rust": "cd src-tauri && cargo doc --no-deps --document-private-items",
    "docs:site": "cd docs && bun run build",
    "docs:site:dev": "cd docs && bun start",
    "docs:build": "bun run docs:api && bun run build-storybook && bun run docs:site",
    "docs:serve": "cd docs && bun run serve",
    "docs:all": "bun run docs:api && bun run docs:rust"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-radio-group": "^1.3.8",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@tanstack/react-query": "^5.90.6",
    "@tanstack/react-query-devtools": "^5.90.2",
    "@tauri-apps/api": "^2.9.0",
    "@tauri-apps/plugin-dialog": "^2.4.2",
    "@tauri-apps/plugin-fs": "^2.4.4",
    "@tauri-apps/plugin-shell": "^2.3.3",
    "axios": "^1.13.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "echarts": "^6.0.0",
    "graphql": "^16.12.0",
    "graphql-request": "^7.3.1",
    "immer": "^11.0.0",
    "katex": "^0.16.25",
    "lucide-react": "^0.552.0",
    "neverthrow": "^8.2.0",
    "next": "^16.0.9",
    "next-themes": "^0.4.6",
    "react": "^19.2.1",
    "react-dom": "^19.2.1",
    "react-grid-layout": "^1.5.2",
    "react-icons": "^5.5.0",
    "react-resizable": "^3.0.5",
    "react-window": "^1.8.11",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7",
    "uplot": "^1.6.32",
    "uplot-react": "^1.2.4",
    "zod": "^4.1.13",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.1.3",
    "@playwright/test": "^1.57.0",
    "@storybook/addon-a11y": "^10.1.10",
    "@storybook/addon-docs": "^10.1.10",
    "@storybook/addon-onboarding": "^10.1.10",
    "@storybook/addon-vitest": "^10.1.10",
    "@storybook/nextjs-vite": "^10.1.10",
    "@tauri-apps/cli": "^2.9.2",
    "@types/node": "^24.9.2",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@types/react-grid-layout": "^1.3.5",
    "@types/react-window": "^1.8.8",
    "@vitest/browser-playwright": "^4.0.14",
    "@vitest/coverage-v8": "^4.0.14",
    "autoprefixer": "^10.4.21",
    "baseline-browser-mapping": "^2.9.11",
    "comlink": "^4.4.2",
    "eslint": "^9.39.0",
    "eslint-config-next": "^16.0.1",
    "eslint-plugin-storybook": "^10.1.10",
    "istanbul-lib-coverage": "^3.2.2",
    "monocart-coverage-reports": "^2.12.9",
    "msw": "^2.12.4",
    "nyc": "^17.1.0",
    "playwright": "^1.57.0",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "storybook": "^10.1.10",
    "tailwindcss": "^3.4.18",
    "typedoc": "^0.28.14",
    "typedoc-plugin-markdown": "^4.9.0",
    "typescript": "^5.9.3",
    "vite": "^7.2.4",
    "vitest": "^4.0.14"
  },
  "overrides": {
    "glob": "^10.5.0",
    "js-yaml": "^4.1.1"
  },
  "resolutions": {
    "glob": "^10.5.0",
    "js-yaml": "^4.1.1"
  }
}
