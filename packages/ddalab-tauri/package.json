{
  "name": "ddalab-tauri",
  "version": "1.0.20",
  "description": "DDALAB Desktop Application built with Next.js and Tauri",
  "private": true,
  "scripts": {
    "dev": "PORT=3003 next dev --turbo",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "tauri": "tauri",
    "tauri:dev": "tauri dev",
    "tauri:dev:serial": "tauri dev",
    "tauri:build": "tauri build",
    "tauri:icon": "tauri icon",
    "fmt": "cd src-tauri && cargo fmt --all",
    "desktop:dev": "npm run tauri:dev",
    "desktop:build": "npm run build && npm run tauri:build",
    "test": "cd src-tauri && cargo test",
    "release": "npm run build && npm run tauri:build && echo 'Build complete! Check src-tauri/target/release/bundle/'",
    "release:mac": "npm run build && tauri build --target universal-apple-darwin",
    "release:test": "./build-test-dmg.sh",
    "release:github": "../../scripts/build-and-release.sh"
  },
  "dependencies": {
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-radio-group": "^1.3.8",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@tanstack/react-query": "^5.90.6",
    "@tanstack/react-query-devtools": "^5.90.2",
    "@tauri-apps/api": "^2.9.0",
    "@tauri-apps/plugin-dialog": "^2.4.2",
    "@tauri-apps/plugin-fs": "^2.4.4",
    "@tauri-apps/plugin-shell": "^2.3.3",
    "axios": "^1.13.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "echarts": "^6.0.0",
    "graphql": "^16.12.0",
    "graphql-request": "^7.3.1",
    "immer": "^10.2.0",
    "lucide-react": "^0.552.0",
    "next": "^16.0.1",
    "next-themes": "^0.4.6",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-grid-layout": "^1.5.2",
    "react-resizable": "^3.0.5",
    "recharts": "^3.3.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7",
    "uplot": "^1.6.32",
    "uplot-react": "^1.2.4",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@tauri-apps/cli": "^2.9.2",
    "@types/node": "^24.9.2",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@types/react-grid-layout": "^1.3.5",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.39.0",
    "eslint-config-next": "^16.0.1",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.18",
    "typescript": "^5.9.3"
  }
}
