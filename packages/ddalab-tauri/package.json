{
  "name": "ddalab-tauri",
  "version": "0.1.0",
  "description": "DDALAB Desktop Application built with Next.js and Tauri",
  "private": true,
  "scripts": {
    "dev": "PORT=3003 next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "tauri": "tauri",
    "tauri:dev": "tauri dev",
    "tauri:build": "tauri build",
    "tauri:icon": "tauri icon",
    "desktop:dev": "npm run tauri:dev",
    "desktop:build": "npm run build && npm run tauri:build",
    "desktop:with-api": "./start-with-api.sh",
    "api:start": "../../scripts/start-api-only.sh up",
    "api:stop": "../../scripts/start-api-only.sh down",
    "api:logs": "../../scripts/start-api-only.sh logs",
    "test": "echo \"No tests specified\" && exit 0",
    "release": "npm run build && npm run tauri:build && echo 'Build complete! Check src-tauri/target/release/bundle/'",
    "release:mac": "npm run build && tauri build --target universal-apple-darwin",
    "release:github": "../../scripts/build-and-release.sh"
  },
  "dependencies": {
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-checkbox": "^1.0.0",
    "@radix-ui/react-dialog": "^1.0.0",
    "@radix-ui/react-dropdown-menu": "^2.0.0",
    "@radix-ui/react-label": "^2.0.0",
    "@radix-ui/react-progress": "^1.0.0",
    "@radix-ui/react-select": "^1.2.0",
    "@radix-ui/react-separator": "^1.0.0",
    "@radix-ui/react-slider": "^1.1.0",
    "@radix-ui/react-slot": "^1.0.0",
    "@radix-ui/react-tabs": "^1.0.0",
    "@radix-ui/react-tooltip": "^1.0.0",
    "@tauri-apps/api": "^2.0.0",
    "axios": "^1.6.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "date-fns": "^3.0.0",
    "lucide-react": "^0.344.0",
    "next": "^15.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-grid-layout": "^1.4.4",
    "react-resizable": "^3.0.5",
    "recharts": "^2.12.0",
    "tailwind-merge": "^2.0.0",
    "uplot": "^1.6.30",
    "uplot-react": "^1.2.2",
    "zustand": "^4.4.0"
  },
  "devDependencies": {
    "@tauri-apps/cli": "^2.0.0",
    "@types/node": "^20.0.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@types/react-grid-layout": "^1.3.0",
    "autoprefixer": "^10.4.0",
    "eslint": "^8.0.0",
    "eslint-config-next": "^15.0.0",
    "postcss": "^8.4.0",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.0.0"
  }
}
