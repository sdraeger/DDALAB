{
  "name": "@ddalab/wasm",
  "version": "0.1.0",
  "description": "WebAssembly module for DDALAB signal processing",
  "main": "pkg/ddalab_wasm.js",
  "module": "pkg/ddalab_wasm.js",
  "types": "pkg/ddalab_wasm.d.ts",
  "files": [
    "pkg"
  ],
  "scripts": {
    "build": "wasm-pack build --target web --out-dir pkg",
    "build:bundler": "wasm-pack build --target bundler --out-dir pkg",
    "build:nodejs": "wasm-pack build --target nodejs --out-dir pkg-node",
    "test": "cargo test",
    "clean": "rm -rf pkg pkg-node target"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/anthropics/ddalab"
  },
  "keywords": [
    "wasm",
    "signal-processing",
    "decimation",
    "lttb",
    "neurophysiology"
  ],
  "license": "MIT",
  "devDependencies": {}
}
